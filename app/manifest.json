{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "background"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "scripts/contentscript.js"
    ],
    "css": [
      "styles/content.css"
    ],
    "run_at": "document_end",
    "all_frames": false
  }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "two minutes",
    "default_popup": "popup.html"
  },
  "commands": {
    "toggle-two-minutes": {
      "suggested_key": {
        "default": "Ctrl+Shift+J",
        "mac": "Command+MacCtrl+J"
      },
      "description": "Toggle Two minutes"
    },
    "toggle-two-minutes-timer": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+MacCtrl+K"
      },
      "description": "Toggle Two minutes timer"
    }
  }
}
